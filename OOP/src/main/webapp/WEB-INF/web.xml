<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
         http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
         
    <servlet>
		<servlet-name>login</servlet-name>
		<servlet-class>IT20281564.AdminLoginServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>packageinfo</servlet-name>
		<servlet-class>IT20281564.PackageInfo</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>packageinfo</servlet-name>
		<url-pattern>/packageDetails</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>insertpackage</servlet-name>
		<servlet-class>IT20281564.InsertPackageServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>insertpackage</servlet-name>
		<url-pattern>/insertpackage</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>updatepackage</servlet-name>
		<servlet-class>IT20281564.UpdatePackServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>updatepackage</servlet-name>
		<url-pattern>/updatepackage</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>deletestaff</servlet-name>
		<servlet-class>IT20281564.DeleteStaffServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>deletestaff</servlet-name>
		<url-pattern>/deletestaff</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>createcustomer</servlet-name>
		<servlet-class>IT20281564.InsertCustomer</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>createcustomer</servlet-name>
		<url-pattern>/createcustomer</url-pattern>
	</servlet-mapping>
         
         
</web-app>